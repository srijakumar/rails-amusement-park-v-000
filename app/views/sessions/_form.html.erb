<%= form_tag url: '/login' do |f| %>

  <div class="field">
    <%= label_tag :name %>
    <%= collection_select(:user, :name, User.all, :name, :name) %>
  </div>

  <div class="field">
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>

  <div class="actions">
    <br><%= submit_tag 'Sign In' %>
  </div>


  </div>

<% end %>
